{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "useJSXTextNode": true,
    "project": "./tsconfig.json",
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint/tslint",
    "@typescript-eslint"
  ],
  "rules": {
    "@typescript-eslint/tslint/config": ["warn", {"lintFile": "./tslint.json"}]
  }
}