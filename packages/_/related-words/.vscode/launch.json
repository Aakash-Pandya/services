{
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "German",
      "program": "${workspaceFolder}/dist/german/index.js",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ],
      "env":{
        "RELATED_WORDS_DEBUG": "true"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "English",
      "program": "${workspaceFolder}/dist/english/index.js",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ],
      "env":{
        "RELATED_WORDS_DEBUG": "true"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Bulgarian",
      "program": "${workspaceFolder}/dist/bulgarian/index.js",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ],
      "env": {
        "PUPPETEER_DEBUG": "true",
        "RELATED_WORDS_DEBUG": "true"
      }
    }
  ]
}