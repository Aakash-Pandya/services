{
  "name": "tag-fn",
  "version": "0.3.6",
  "description": "CLI command for lazy generation of Github tags",
  "main": "dist/index.ts",
  "typings": "index.d.ts",
  "repository": "git@github.com:selfrefactor/tag-fn.git",
  "author": "Dejan Toteff <deyan8284@gmail.com>",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "fs-extra": "^6.0.1",
    "init-puppeteer": "https://github.com/selfrefactor/init-puppeteer#6.8.1",
    "inquirer": "^6.2.0",
    "helpers": "https://github.com/selfrefactor/helpers#1.2.1",
    "rambdax": "2.10.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^5.0.3",
    "@types/jest": "^23.1.3",
    "@types/node": "^10.5.0",
    "@types/puppeteer": "^1.12.4",
    "env-fn": "^1.1.1",
    "jest": "^23.2.0",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "typescript": "^3.5.0"
  },
  "scripts": {
    "debug": "DEBUG_FLAG=true node debug.js",
    "test": "jest",
    "in": "tsc -w",
    "out": "tsc -p .",
    "lint": "tslint --type-check --project tsconfig.json"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "preferGlobal": true,
  "bin": {
    "tag": "bin/tag"
  },
  "directories": {
    "bin": "./bin"
  },
  "commitMessage": {
    "workInProgress": ""
  }
}
